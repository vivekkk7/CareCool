    <script>
        let submitbtn = document.getElementById('submit_btn');
        let aptForm = document.getElementById('apt_form');

        aptForm.addEventListener('submit', function (event) {
            event.preventDefault();

            submitbtn.disabled = true;
            aptForm.style.opacity = '0.7';
            $('#sending').show();
            $(submitbtn).hide();

            let form4 = event.target;
            let formData4 = new FormData(form4);

            fetch('/', {
                method: 'POST',
                body: formData4
            })
                .then(response => response.json())
                .then(data => {
                    console.log('s1');
                    if (data.isappoint == true) {
                       
                        $('#sending').hide();
                        $(submitbtn).show().val('Submitted');
                        showAlert("Form submitted successfully. Redirecting to profile.", "positive");
                        const expires = new Date();
                        expires.setDate(expires.getDate() + 365);
                        document.cookie = `userid=${data.cookie}; expires=${expires.toUTCString()}; SameSite=Lax; path=/`;

                        setTimeout(() => {
                            window.location.href = "/profile";
                        }, 2000);
                    }
                    else if (data.isappoint == false) {
                        
                        showAlert(data.aerror, "negative");
                        $('#sending').hide();
                        $(submitbtn).show()

                        if (data.cdelete == true) {
                            document.cookie = "username=uu; expires=Thu, 18 Dec 2013 12:00:00 UTC; path=/";
                        }
                        submitbtn.disabled = false;
                        aptForm.style.opacity = '1';
                    }
                    else {
                        
                        $('#sending').hide();
                        showAlert("Something went wrong. Please try again.", "negative");
                        $(submitbtn).show()
                        submitbtn.disabled = false;
                        aptForm.style.opacity = '1';
                    }
                })
                .catch(error => {
                    console.log("Form error : " + error);
                    
                    $('#sending').hide();
                    showAlert("Oops! Something went wrong. Refreshing page. Please try again.", "negative");
                    $(submitbtn).show()
                    submitbtn.disabled = false;
                    aptForm.style.opacity = '1';
                });
        });

        
    </script>


